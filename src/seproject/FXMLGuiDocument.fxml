<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="448.0" prefWidth="539.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seproject.FXMLGuiDocumentController">
   <children>
      <VBox fx:id="menuVBox" prefHeight="127.0" prefWidth="589.0">
         <children>
            <MenuBar>
              <menus>
                <Menu fx:id="fileMenu" mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem fx:id="saveMenu" mnemonicParsing="false" onAction="#handleActionSaveDrawing" text="Save" />
                        <MenuItem fx:id="loadMenu" mnemonicParsing="false" onAction="#handleActionLoadDrawing" text="Load" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <HBox fx:id="toolBox" prefHeight="105.0" prefWidth="589.0" spacing="10.0">
               <children>
                  <Button fx:id="rettangleButton" mnemonicParsing="false" onAction="#handleButtonActionRectangle" text="Rectangle" />
                  <Button fx:id="ellipseButton" mnemonicParsing="false" onAction="#handleButtonActionEllipse" text="Ellipse" />
                  <Button fx:id="lineButton" mnemonicParsing="false" onAction="#handleButtonActionLine" text="Line" />
               </children>
               <padding>
                  <Insets left="10.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
      <ScrollPane layoutY="128.0" prefHeight="320.0" prefWidth="589.0">
        <content>
          <AnchorPane fx:id="drawingArea" minHeight="0.0" minWidth="0.0" onMousePressed="#handleMousePressedOnDrawingArea" onMouseReleased="#handleMouseReleasedOnDrawingArea" prefHeight="2000.0" prefWidth="2000.0" />
        </content>
      </ScrollPane>
   </children>
</AnchorPane>
