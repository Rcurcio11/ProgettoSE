<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="448.0" prefWidth="539.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seproject.FXMLGuiDocumentController">
   <children>
      <ScrollPane layoutY="128.0" prefHeight="320.0" prefWidth="589.0">
        <content>
          <AnchorPane fx:id="drawingArea" minHeight="0.0" minWidth="0.0" onMouseClicked="#handleMouseClickedOnDrawingArea" onMouseReleased="#handleMouseReleasedOnDrawingArea" prefHeight="2000.0" prefWidth="2000.0" />
        </content>
      </ScrollPane>
      <HBox fx:id="toolBox" prefHeight="130.0" prefWidth="589.0">
         <children>
            <Button fx:id="rettangleButton" mnemonicParsing="false" onAction="#handleButtonActionRectangle" text="Rectangle" />
            <Button fx:id="ellipseButton" mnemonicParsing="false" onAction="#handleButtonActionEllipse" text="Ellipse" />
         </children></HBox>
   </children>
</AnchorPane>
